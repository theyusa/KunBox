        android:fillColor="#00000000"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="6"
        android:strokeLineJoin="round"
        android:strokeLineCap="round"
        android:pathData="M54,36 L72,45 L72,63 L54,72 L36,63 L36,45 Z" />
        
    <!-- Inner Y shape to define the 3 faces -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="6"
        android:strokeLineJoin="round"
        android:strokeLineCap="round"
        android:pathData="M54,54 L54,72 M54,54 L36,45 M54,54 L72,45" />
        
    <!-- "S" stylized inside the box faces? Or maybe just a clean box is better. -->
    <!-- Let's add a subtle detail to make it look like a package/singbox -->
    
